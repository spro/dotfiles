let colors_name = "spro"

hi Identifier cterm=NONE ctermfg=81
hi Operator ctermfg=229
hi Conditional cterm=bold ctermfg=11
hi String ctermfg=252
hi Comment ctermfg=245
hi Statement ctermfg=111
hi Structure cterm=bold ctermfg=110
hi Function cterm=bold ctermfg=87
hi Constant cterm=bold ctermfg=210
hi Label ctermfg=122
hi Include ctermfg=103
" hi Variable

hi coffeeObjAssign cterm=bold ctermfg=159

hi MatchParen ctermfg=white ctermbg=245
hi StatusLine ctermfg=238 ctermbg=231
